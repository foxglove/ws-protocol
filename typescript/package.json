{
  "name": "@foxglove/ws-protocol",
  "version": "0.0.5",
  "description": "Foxglove Studio WebSocket protocol client",
  "keywords": [
    "foxglove",
    "websocket",
    "robotics",
    "ros",
    "ros2"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/foxglove/ws-protocol.git"
  },
  "author": {
    "name": "Foxglove Technologies",
    "email": "support@foxglove.dev"
  },
  "homepage": "https://foxglove.dev/",
  "module": "dist/esm/src/index.js",
  "main": "dist/cjs/src/index.js",
  "typings": "dist/esm/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "bin": {
    "example-sysmon": "dist/cjs/examples/sysmon.js",
    "example-simple-client": "dist/cjs/examples/simple-client.js"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "tsc -b tsconfig.json tsconfig.cjs.json",
    "lint": "eslint --report-unused-disable-directives --fix .",
    "lint:ci": "eslint --report-unused-disable-directives .",
    "test": "jest",
    "example:image-server": "ts-node --project tsconfig.cjs.json --files examples/image-server.ts",
    "example:sysmon": "ts-node --project tsconfig.cjs.json --files examples/sysmon.ts",
    "example:simple-client": "ts-node --project tsconfig.cjs.json --files examples/example-client.ts"
  },
  "devDependencies": {
    "@foxglove/eslint-plugin": "0.17.1",
    "@foxglove/rostime": "1.1.1",
    "@foxglove/tsconfig": "1.1.0",
    "@types/debug": "4.1.7",
    "@types/jest": "^27.0.2",
    "@types/lodash": "4.14.176",
    "@types/ws": "8.2.0",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "commander": "8.3.0",
    "consumer-queue": "^1.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-es": "4.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.2.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.3.1",
    "lodash": "4.17.21",
    "prettier": "2.4.1",
    "protobufjs": "6.11.2",
    "pureimage": "0.3.6",
    "ts-jest": "27.0.7",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
    "ws": "8.2.3"
  },
  "dependencies": {
    "debug": "^4",
    "eventemitter3": "^4.0.7",
    "tslib": "^2"
  }
}
